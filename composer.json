{
    "name": "Danil005/botman-vk-driver",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Danil005/botman-vk-driver",
    "description": "Vk driver for BotMan",
    "keywords": [
        "BotMan",
        "Vk"
